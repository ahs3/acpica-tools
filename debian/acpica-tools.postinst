#!/bin/sh
set -e

BINDIR=/usr/bin
MANDIR=/usr/share/man/man1
update-alternatives --install $BINDIR/acpixtract acpixtract \
                              $BINDIR/acpixtract-acpica 100 \
                    --slave $MANDIR/man1/acpixtract.1.gz acpixtract.1.gz \
                            $MANDIR/man1/acpixtract-acpica.1

