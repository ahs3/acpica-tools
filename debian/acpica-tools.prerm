#!/bin/sh
set -e

BINDIR=/usr/bin
case $1 in
remove) if [ ! -e $BINDIR/acpixtract-acpica ]
	then
    	    update-alternatives --remove acpixtract $BINDIR/acpixtract-acpica
	fi
	if [ ! -e $BINDIR/acpidump-acpica ]
	then
    	    update-alternatives --remove acpidump $BINDIR/acpidump-acpica
	fi
	;;
*)	;;
esac

#DEBHELPER#

exit 0
